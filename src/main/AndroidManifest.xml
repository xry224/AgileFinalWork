                 android:icon="@mipmap/ic_launcher"
                 android:roundIcon="@mipmap/ic_launcher_round"
                 android:supportsRtl="true"
                 android:theme="@style/Theme.AppCompat.Light.NoActionBar"
                 tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="view.findNearby">
        </activity>
        <activity android:name="view.historyList">
        </activity>
        <activity android:name="view.myAccount">
        </activity>
        <activity
                android:name="view.MainActivity"
                android:label="出游" >
         </activity>
        <activity android:name="view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="view.EventDetail"/>
        <activity android:name="view.newEventActivity"/>
        <activity android:name="view.Register"/>
        <activity android:name="view.ForgetPW"/>
        <activity android:name="view.MerchantDetail"/>
        <activity android:name="view.MessageActivity"/>
    </application>
</manifest>
